out_to_x              no
own_window            no
out_to_console        yes
background            no
max_text_width        0

update_interval       2.0
total_run_times       0
short_units           yes

if_up_strictness      address

use_spacer            left
override_utf8_locale  no
cpu_avg_samples       2

TEXT

 [
  {
    "full_text" : "${battery_short} ${battery_percent}%" , 
    "color" : "\#979797"
  },
  {
    "full_text" : "${if_up wlp3s0}${addr enp0s10}${endif}" ,
    "color" : "\#979797" 
  },
  {
    "full_text" : "${if_up enp0s10}${addr enp0s10}${endif}" ,
    "color" : "\#979797"
  },
  {
    "full_text" : "${time %a %d %b %H:%M}",
    "color" : "\#979797"
  },
 ],


