background true

out_to_x no
out_to_console yes

update_interval 1

TEXT
L ${if_up enp0s10}${addr enp0s10}${else}DOWN${endif} | \
W ${if_up wlp3s0}${addr wlp3s0}${else}DOWN${endif} | \
B ${battery_percent BAT0}% | \
CPU1 ${cpu cpu1}% | CPU2 ${cpu cpu2}% | \
D ${head $HOME/scripts/curws 1} | \
G [${execi 60 $HOME/scripts/gmail}]
